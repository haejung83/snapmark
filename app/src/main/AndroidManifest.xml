<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.haejung.snapmark">

  <application
      android:name=".SnapMarkApplication"
      android:allowBackup="false"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/Theme.Shrine">

    <activity android:name=".presentation.snap.SnapActivity">
    </activity>

    <activity android:name=".presentation.addmark.AddMarkActivity">
    </activity>

    <activity android:name=".presentation.main.MainActivity">
    </activity>

    <activity android:name=".presentation.intro.IntroActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

  </application>

</manifest>